<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schorndorf_invaders.FXMLDocumentController">
    <children>
        <Pane fx:id="canvas" maxHeight="-1.0" maxWidth="-1.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="400.0">
            <children>
                <Circle fx:id="circle" fill="DODGERBLUE" radius="50.0" stroke="BLACK" strokeType="INSIDE" />
            </children>
        </Pane>
        <HBox alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="400.0" spacing="5.0">
            <children>
                <Button id="button" onAction="#handleMoveAction" text="Move" />
                <Button mnemonicParsing="false" onAction="#handleStoppAction" text="Stopp" />
                <Button id="handleColor" mnemonicParsing="false" onAction="#handleColorAction" text="Color" />
            </children>
        </HBox>
    </children>
</AnchorPane>
